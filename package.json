    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.1",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@types/whatwg-fetch": "^0.0.33",
    "@vitejs/plugin-react": "^4.3.4",
